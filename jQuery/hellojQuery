        <script type="text/javascript" src="js/jquery-3.6.0.min.js"></script>
        <script type="text/javascript">
            $(document).ready(function(){
                alert("hello jquery")
            })
            $(function(){
                alert("Hello jQuery")
            })
        </script>
    </head>
